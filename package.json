{
  "name": "@bifravst/gcp",
  "version": "0.0.0-development",
  "description": "Google Cloud Platform implementation of Bifravst",
  "scripts": {
    "postinstall": "check-node-version --package && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bifravst/gcp.git"
  },
  "bugs": {
    "url": "https://github.com/Bifravst/gcp/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@bifravst/device-ui-server": "1.3.2",
    "@bifravst/random-words": "4.2.0",
    "deepmerge": "4.2.2",
    "googleapis": "49.0.0",
    "jsonwebtoken": "8.5.1",
    "mqtt": "3.0.0"
  },
  "devDependencies": {
    "@bifravst/code-style": "7.1.11",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-angular": "9.1.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "13.13.0",
    "check-node-version": "4.0.3",
    "husky": "4.2.5",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npx tsc"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
